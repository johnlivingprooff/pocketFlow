{
  "expo": {
    "name": "pocketFlow",
    "slug": "pocketflow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#B3B09E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon.png",
        "backgroundColor": "#B3B09E"
      },
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "package": "com.eiteone.pocketflow"
    },
    "web": {
      "favicon": "./assets/app_icon.png",
      "bundler": "webpack"
    },
    "scheme": "pocketflow",
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow pocketFlow to use Face ID."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e63eeb3a-bc99-4112-b2c0-d0568d5593d3"
      }
    },
    "owner": "john-livingprooff"
  }
}
