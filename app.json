{
  "expo": {
    "name": "pocketFlow",
    "slug": "pocketflow",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0FDFA",
      "dark": {
        "image": "./assets/logo.png",
        "backgroundColor": "#041C1F"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eiteone.pocketflow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon.png",
        "backgroundColor": "#F0FDFA"
      },
      "permissions": [
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "package": "com.eiteone.pocketflow"
    },
    "web": {
      "favicon": "./assets/app_icon.png"
    },
    "scheme": "pocketflow",
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow pocketFlow to use Face ID."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#84670B",
          "sounds": []
        }
      ],
      "@react-native-community/datetimepicker",
      "expo-secure-store"
    ],
    "extra": {
      "router": {},
      "enableLogging": true,
      "cloudApiBaseUrl": "https://cloud.pf.eiteone.org",
      "eas": {
        "projectId": "e63eeb3a-bc99-4112-b2c0-d0568d5593d3"
      }
    },
    "owner": "john-livingprooff"
  }
}
